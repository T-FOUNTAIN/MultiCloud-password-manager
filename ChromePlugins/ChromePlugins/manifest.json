{
	"manifest_version": 2, 
	"name": "MultiCloudPasswordManager",
	"version": "1.0.0",
	"description": "Security Password Storage by MultiCloud",
	"icons":{
		"16":"img/icon.png",
		"48":"img/icon.png",
		"128":"img/icon.png"
	},
	"browser_action":{
		"default_icon":{
			"19": "img/icon.png",
			"38": "img/icon.png"
		},
		"default_title": "MultiCloudPasswordManager",
		"default_popup": "popup.html"
	},
	"permissions":[
		"http://*/"
	]
}
